
.bar-icon{
		width: 55rpx;
		position: relative;
		display: block;
		height: 55rpx;
		font-size: 30rpx;
}
.bar-circle {
		position: relative;
		display: block;
		margin-bottom: 55rpx;
		line-height: 120rpx;
		width: 120rpx !important;
		height: 120rpx !important;
		overflow: hidden;
		border-radius: 50%;
		box-shadow: 0rpx 0rpx 20rpx 0rpx rgba(69, 156, 26, 0.3);
}

